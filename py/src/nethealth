#!/usr/local/bin/python3

if __name__ == '__main__':
    from controllers import OperationsController
    from operations import DnsLookupOp, PingOp

    operations_controller = OperationsController()
    operations_controller.operations = [
        DnsLookupOp('accezz.io'),
        DnsLookupOp('yahoo.com'),
        PingOp('http://download.thinkbroadband.com/5MB.zip')
    ]
    operations_controller.run()